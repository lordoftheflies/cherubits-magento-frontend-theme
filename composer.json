{
    "name": "cherubits/cherubits-magento-frontend-theme",
    "description": "Frontend theme for Cherubits webshop",
    "require": {
        "php": "~7.0.0",
        "magento/theme-frontend-blank": "100.0.*",
        "magento/framework": "100.0.*",
        "magento/magento-composer-installer": "*"
    },
    "type": "magento2-theme",
    "version": "100.0.1",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ]
    },
    "extra": {
        "map": [
            [
                "*",
                "frontend/Cherubits/default"
            ]
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lordoftheflies/cherubits-magento-frontend-theme.git"
        }
    ]
}
